        },
        {
            "path": "lwyj123-roosterjs-editor-dom/tsconfig.child.json"
        },
        {
            "path": "lwyj123-roosterjs-editor-core/tsconfig.child.json"
        },
        {
            "path": "lwyj123-roosterjs-editor-api/tsconfig.child.json"
        },
        {
            "path": "lwyj123-roosterjs-editor-plugins/tsconfig.child.json"
        },
        {
            "path": "lwyj123-roosterjs-color-utils/tsconfig.child.json"
        },
        {
            "path": "lwyj123-roosterjs/tsconfig.child.json"
        },
        {
