{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "baseUrl": "../../",
        "lib": ["ES2017", "dom"],
        "jsx": "react",
        "sourceMap": true,
        "removeComments": true,
        "noImplicitAny": true,
        "preserveConstEnums": false,
        "noUnusedLocals": true,
