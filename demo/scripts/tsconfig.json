            "lwyj123-roosterjs-content-model": ["packages/lwyj123-roosterjs-content-model/lib/index"],
            "lwyj123-roosterjs-content-model/lib/*": ["packages/lwyj123-roosterjs-content-model/lib/*"]
        }
    },
    "include": ["./**/*.ts", "./**/*.tsx"]
