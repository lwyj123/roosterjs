            "lwyj123-roosterjs-editor-core": ["packages/lwyj123-roosterjs-editor-core/lib/index"],
            "lwyj123-roosterjs-editor-core/lib/*": ["packages/lwyj123-roosterjs-editor-core/lib/*"],
            "lwyj123-roosterjs-editor-api": ["packages/lwyj123-roosterjs-editor-api/lib/index"],
            "lwyj123-roosterjs-editor-api/lib/*": ["packages/lwyj123-roosterjs-editor-api/lib/*"],
